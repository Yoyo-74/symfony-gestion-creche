{% extends 'base.html.twig' %}

{% block title %}Responsables{% endblock %}

{% block body %}
    <h1>Responsables</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ responsable.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ responsable.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ responsable.prenom }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ responsable.email }}</td>
            </tr>
            <tr>
                <th>Tel</th>
                <td>{{ responsable.tel }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_responsables_index') }}">back to list</a>

    <a href="{{ path('app_responsables_edit', {'id': responsable.id}) }}">edit</a>

    {{ include('responsables/_delete_form.html.twig') }}
